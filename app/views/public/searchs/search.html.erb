<div class = "container">
    <% if @range == "Camper" %>
        <% @campers.each do |camper| %>
          <%= link_to camper_path(camper),class: "black-color no-text-decoration",
           title: camper.camper_name do %>
          <string><%= camper.camper_name %></string>
          <% end %>
        <% end %>
    <% else %>
        <% @posts.each do |post| %>
           <%= link_to camper_path(post),class: "black-color no-text-decoration",
           title: post.camper.camper_name do %>
           <string><%= post.camper.camper_name %></string>
           <% end %>
            <%= post.caption %>
            <%= image_tag post.photos.first.image.url(:medium), :size =>'200x200', class: "ml-auto mx-0 my-auto" do %>
            <div class="delete-post-icon">
            </div>
            <% end %>
        <% end %>
<% end %>
</div>